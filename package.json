{
  "name": "photoblog-hugo",
  "private": true,
  "scripts": {
    "start": "hugo server -D",
    "prebuild": "rm -rf ./dist",
    "build": "hugo --minify",
    "fetch-insta": "ts-node scripts/fetch-insta.ts",
    "lint": "eslint --quiet --ignore-path .gitignore ./{scripts,assets}/**/*.ts postcss.config.js",
    "lint:fix": "npm run lint -- --fix",
    "fmt": "prettier --config ./.prettierrc --ignore-path ./.prettierignore .",
    "fmt:check": "npm run fmt -- --check",
    "fmt:write": "npm run fmt -- --write",
    "tsc": "tsc --noEmit",
    "test": "npm-run-all lint tsc fmt:check"
  },
  "devDependencies": {
    "@types/node": "18.11.12",
    "@types/turbolinks": "5.2.0",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.19.6",
    "@babel/preset-env": "7.19.4",
    "@fortawesome/fontawesome-free": "5.15.4",
    "autoprefixer": "10.4.13",
    "contentful": "9.2.9",
    "cssnano": "5.1.14",
    "dotenv": "16.0.3",
    "emoji-regex": "10.2.1",
    "postcss": "8.4.18",
    "postcss-cli": "10.0.0",
    "slugify": "1.6.5",
    "turbolinks": "5.2.0"
  }
}
