{
  "name": "photoblog-hugo",
  "private": true,
  "scripts": {
    "start": "hugo server -D",
    "prebuild": "rm -rf ./dist",
    "build": "hugo --minify",
    "fetch-insta": "ts-node scripts/fetch-insta.ts",
    "lint": "eslint --quiet --ignore-path .gitignore ./{scripts,assets}/**/*.ts postcss.config.js",
    "lint:fix": "npm run lint -- --fix",
    "fmt": "prettier --config ./.prettierrc --ignore-path ./.prettierignore .",
    "fmt:check": "npm run fmt -- --check",
    "fmt:write": "npm run fmt -- --write",
    "tsc": "tsc --noEmit",
    "test": "npm-run-all lint tsc fmt:check"
  },
  "devDependencies": {
    "@types/node": "18.18.13",
    "@types/turbolinks": "5.2.2",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "3.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.3.2"
  },
  "dependencies": {
    "@babel/cli": "7.23.0",
    "@babel/core": "7.23.0",
    "@babel/preset-env": "7.22.20",
    "@fortawesome/fontawesome-free": "6.4.2",
    "autoprefixer": "10.4.16",
    "contentful": "9.3.5",
    "cssnano": "6.0.1",
    "dotenv": "16.3.1",
    "emoji-regex": "10.2.1",
    "postcss": "8.4.31",
    "postcss-cli": "10.1.0",
    "slugify": "1.6.6",
    "turbolinks": "5.2.0"
  }
}
