{
  "name": "photoblog-hugo",
  "private": true,
  "scripts": {
    "start": "hugo server -D",
    "prebuild": "rm -rf ./dist",
    "build": "hugo --minify",
    "fetch-insta": "ts-node scripts/fetch-insta.ts",
    "lint": "eslint --quiet --ignore-path .gitignore ./{scripts,assets}/**/*.ts postcss.config.js",
    "lint:fix": "npm run lint -- --fix",
    "fmt": "prettier --config ./.prettierrc --ignore-path ./.prettierignore .",
    "fmt:check": "npm run fmt -- --check",
    "fmt:write": "npm run fmt -- --write",
    "tsc": "tsc --noEmit",
    "test": "npm-run-all lint tsc fmt:check"
  },
  "devDependencies": {
    "@types/node": "14.17.1",
    "@types/turbolinks": "5.2.0",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "eslint": "7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  },
  "dependencies": {
    "@babel/cli": "7.14.3",
    "@babel/core": "7.14.3",
    "@babel/preset-env": "7.14.2",
    "@fortawesome/fontawesome-free": "5.15.3",
    "autoprefixer": "10.2.5",
    "contentful": "8.3.7",
    "cssnano": "5.0.4",
    "dotenv": "10.0.0",
    "emoji-regex": "9.2.2",
    "postcss": "8.3.0",
    "postcss-cli": "8.3.1",
    "slugify": "1.5.3",
    "turbolinks": "5.2.0"
  }
}
